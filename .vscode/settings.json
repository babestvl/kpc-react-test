{
  "editor.tabSize": 2,
  "editor.insertSpaces": false,
  "search.exclude": {
    "**/node_modules": true,
  },
  "path-intellisense.mappings": {
		"pages": "${workspaceRoot}/src/pages",
		"common": "${workspaceRoot}/src/common"
	},
  "javascript.validate.enable": false,
}
